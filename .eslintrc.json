{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "react-hooks/exhaustive-deps": "warn"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "settings": {
    "next": {
      "rootDir": "."
    }
  }
}
