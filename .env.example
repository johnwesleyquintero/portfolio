#=============================================================================
# API Keys and Authentication
#=============================================================================
GEMINI_API_KEY=your_gemini_api_key_here
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token_here

#=============================================================================
# Vercel Configuration
#=============================================================================
# Blob Storage
BLOB_READ_WRITE_TOKEN="your_blob_read_write_token_here"

# Edge Configuration
EDGE_CONFIG=https://edge-config.vercel.com/your_config_id?token=your_token_here
EDGE_CONFIG_STORE=your_edge_config_store_id_here

# Project Details
VERCEL_PROJECT_ID=your_project_id_here
VERCEL_TEAM_ID=your_team_id_here
VERCEL_TOKEN=your_vercel_token_here

#=============================================================================
# Environment Configuration
#=============================================================================
NODE_ENV=development

# Valid NODE_ENV values:
# - development
# - staging
# - production
# - test

#=============================================================================
# GitHub Configuration
#=============================================================================
CALL_BACK=https://github.com/settings/connections/applications/your_client_id_here
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here
PAT=your_personal_access_token_here
REPO_URL=https://github.com/yourusername/yourrepo
USERNAME=yourusername

#=============================================================================
# Statsig Configuration
#=============================================================================
EXPERIMENTATION_CONFIG=https://edge-config.vercel.com/your_config_id?token=your_token_here
EXPERIMENTATION_CONFIG_ITEM_KEY="your_config_item_key_here"
NEXT_PUBLIC_STATSIG_CLIENT_KEY="your_statsig_client_key_here"
STATSIG_SERVER_API_KEY="your_statsig_server_api_key_here"
